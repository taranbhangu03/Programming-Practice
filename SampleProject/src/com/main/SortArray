package August;

import java.util.Arrays;

public class SortArray {

	public static void main(String[] args) {
		
		int[]arrNum=new int[6];
		arrNum[0]=50;
		arrNum[1]=13;
		arrNum[2]=30;
		arrNum[3]=10;
		arrNum[4]=34;
		arrNum[5]=38;
		
		Arrays.sort(arrNum);
		int i;
		for(i=0;i<arrNum.length;i++){
			System.out.println("num:"+arrNum[i]);
		}
		
		

	}

}
