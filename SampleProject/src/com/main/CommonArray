package August;

public class CommonArray {

	
	public static void main(String[] args) {
	   int[] arr1={4,6,77,33,11,36};
	   int[] arr2={11,22,6,77,33,56,90,5};
	   for(int i=0;i<=arr1.length;i++){
		   for(int j=0;j<=arr1.length;j++){
			   if(arr1[i]==arr2[j]){
				   System.out.println(arr1[i]);
			   }
		   }
	   }

	}

}
